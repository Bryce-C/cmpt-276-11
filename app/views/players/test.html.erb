<h4> test: <%= @result %> </h4>
<div class="table-responsive">
<table class="table table-hover table-condensed col-md-3">
    <thead>
        <tr >
            <th>Date</th>
						<th>Matchup</th>
            <th>MIN</th>
            <th>PTS</th>
            <th>REB</th>
            <th>AST</th>
        </tr>
    </thead>
    <tbody>
					<% @result.each do |game| %>
					<tr>
						<td> <%= game["GAME_DATE"] %> </td>
						<td> <%= game["MATCHUP"] %> </td>
            <td> <%= game["MIN"] %> </td>
            <td> <%= game["PTS"] %> </td>
            <td> <%= game["REB"] %> </td>
            <td> <%= game["AST"] %> </td>
					</tr>
					<% end %>
    </tbody>
</table>
</div>
